    <declare-styleable name="CircleChartView">
        <attr name="circle_width" format="dimension"/>
        <attr name="back_circle_color" format="color"/>
        <attr name="out_circle_color" format="color"/>
        <attr name="center_circle_color" format="color"/>
        <attr name="inner_circle_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="MaterialEditText">
        <attr name="useFloatingLabel" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="FlowLayout">
        <attr name="flFlow" format="boolean"/>
        <attr name="flChildSpacing" format="enum|dimension">
            <enum name="auto" value="-65536"/>
        </attr>
        <attr name="flMinChildSpacing" format="dimension"/>
        <attr name="flChildSpacingForLastRow" format="enum|dimension">
            <enum name="auto" value="-65536"/>
            <enum name="align" value="-65537"/>
        </attr>
        <attr name="flRowSpacing" format="enum|dimension">
            <enum name="auto" value="-65536"/>
        </attr>
        <attr name="flRtl" format="boolean"/>
        <attr name="flMaxRows" format="integer"/>
        <attr name="flRowVerticalGravity" format="enum">
            <enum name="auto" value="-65536"/>
            <enum name="top" value="0x30"/>
            <enum name="center" value="0x10"/>
            <enum name="bottom" value="0x50"/>
        </attr>
        <attr name="android:gravity"/>
    </declare-styleable>

    <declare-styleable name="ComponentCustomAttrs">
        <attr name="custom_theme_color" format="color"/>
        <attr name="custom_theme_start_color" format="color"/>
        <attr name="custom_theme_end_color" format="color"/>

        <attr name="custom_normal_background_color" format="color"/>
        <attr name="custom_disable_background_color" format="color"/>

        <attr name="custom_theme_text_color" format="color"/>
        <attr name="custom_normal_text_color" format="color"/>
        <attr name="custom_disable_text_color" format="color"/>
        <attr name="custom_hint_text_color" format="color"/>

        <attr name="custom_hint_background_color" format="color"/>
        <attr name="custom_radius" format="dimension"/>

    </declare-styleable>

    <declare-styleable name="SegmentedControlView">
        <attr name="segRadius" format="dimension|reference" />
        <attr name="segOuterColor" format="color" />
        <attr name="segItemColor" format="color" />
        <attr name="segTextColor" format="color" />
        <attr name="segShaderStartColor" format="color" />
        <attr name="segShaderEndColor" format="color" />
        <attr name="segSelectedTextColor" format="color" />
        <attr name="segMarginLeft" format="dimension|reference"/>
        <attr name="segMarginTop" format="dimension|reference"/>
        <attr name="segSelectedItem" format="integer"/>
        <attr name="segTextSize" format="dimension|reference"/>
        <attr name="segScrollEnable" format="boolean"/>
        <attr name="segMode">
            <enum name="ROUND" value="0"/>
            <enum name="CIRCLE" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="CircularProgressView">
        <attr name="cpv_color" format="color"/>
        <attr name="cpv_thickness" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CircleProgressBar">
        <attr name="circleProgressBarTextColor" format="color"/>
        <attr name="circleProgressBarProgressColor" format="color"/>
        <attr name="circleProgressBarThickness" format="dimension"/>
        <attr name="circleProgressBarTextSize" format="dimension"/>
        <attr name="circleProgressBarShowText" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="HorizontalLoadingView">
        <attr name="horizontalLoadingViewLineWidth" format="dimension"/>
        <attr name="horizontalLoadingViewBackgroundColor" format="color"/>
        <attr name="horizontalLoadingViewSliderColor" format="color"/>
        <attr name="horizontalLoadingViewSliderWidth" format="dimension"/>
    </declare-styleable>

</resources>